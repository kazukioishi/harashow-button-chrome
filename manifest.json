{
  "name": "ハラショー拡張",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Facebookのいいね!ボタンをハラショーボタンに置き換える拡張",
  "permissions": [
    "https://www.facebook.com/*"
  ],
  "content_scripts": [
  {
    "matches": ["https://www.facebook.com/*"],
    "js": ["jquery-2.1.1.min.js","script.js"],
    "run_at": "document_start"
  }
  ]
}